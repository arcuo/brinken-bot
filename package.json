  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@types/bun": "^1.2.8",
    "@types/luxon": "^3.6.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-expect-message": "^1.1.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ]
  },
  "packageManager": "yarn@4.5.1"
}